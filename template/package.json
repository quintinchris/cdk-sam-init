{
  "name": "template",
  "version": "0.0.1",
  "bin": {
    "template": "bin/template.js"
  },
  "scripts": {
    "start": "sam local start-api",
    "prebuild": "rm -rf build",
    "build": "tsc && sam build",
    "build-template": "cdk synth --profile default --no-staging --no-version-reporting --path-metadata false > template.yaml",
    "build-all": "npm run build-template && npm run build",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "aws-sdk-client-mock": "^2.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "20.7.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "aws-cdk": "2.101.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.188.0",
    "@aws-sdk/lib-dynamodb": "^3.188.0",
    "aws-cdk-lib": "2.101.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
